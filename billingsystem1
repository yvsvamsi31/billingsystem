	void cosmetics()
	{
		int value;
		System.out.println(
		"-------------------------------------------------------------------------------------------------------------------");
		System.out.println(" 301   Kajal------->1-------------->10");
		System.out.println(" 302   Lipstick---->1-------------->20");
		System.out.println(" 303   Nail Polish->1-------------->50");
		System.out.println(" 304   Face Powder->1-------------->120");
		System.out.println(
		"-------------------------------------------------------------------------------------------------------------------");
		System.out.println("Please choose your product code");
		int code=sc.nextInt();
		switch(code)
		{
			case 301:
			{
				String name="Kajal";
	            value=10;
	            System.out.println("how many units do u want?");
	    		int kgs=sc.nextInt();
	    		switch(kgs)
	    		{
	    		case 1:
	    		{
	    			int a;
	    			 
	    			for(a=1;a<2;)
	    			{
