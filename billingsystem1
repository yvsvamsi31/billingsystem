	void vegetables()
	{
		int value;
		System.out.println(
		"-------------------------------------------------------------------------------------------------------------------");
		System.out.println(" 201   Tomato------>1kg-------------->60");
		System.out.println(" 202   Spinach----->1kg-------------->20");
		System.out.println(" 203   Beetroot---->1kg-------------->50");
		System.out.println(" 204   Potato------>1kg-------------->35");
		System.out.println(" 205   Brinjal----->1kg-------------->45");
		System.out.println(
		"-------------------------------------------------------------------------------------------------------------------");
		System.out.println("Please choose your product code");
		int code=sc.nextInt();
		switch(code)
		{
			case 201:
			{
				String name="Tomato";
	            value=60;
	            System.out.println("how many kgs do u want?");
	    		int kgs=sc.nextInt();
	    		switch(kgs)
	    		{
	    		case 1:
	    		{
	    			int a;
	    			 
	    			for(a=1;a<2;)
	    			{
	    				val[i]=60;
	    				i++;
	    				a++;
	    			}
