	void masalaPowders()
	{
		int value;
		System.out.println(
		"-------------------------------------------------------------------------------------------------------------------");
		System.out.println(" 401   Turmeric Powder------>1kg-------------->330");
		System.out.println(" 402   Garam Masala--------->1kg-------------->600");
		System.out.println(" 403   Sambar Powder-------->1kg-------------->360");
		System.out.println(" 404   Ginger Powder-------->1kg-------------->350");
		System.out.println(" 405   Chicken Masala------->1kg-------------->370");
		System.out.println(
		"-------------------------------------------------------------------------------------------------------------------");
		System.out.println("Please choose your product code");
		int code=sc.nextInt();
		switch(code)
		{
			case 401:
			{
				String name="Turmeric Powder";
	            value=330;
	            System.out.println("how many kgs do u want?");
	    		int kgs=sc.nextInt();
	    		switch(kgs)
	    		{
	    		case 1:
	    		{
	    			int a;
	    			 
	    			for(a=1;a<2;)
	    			{
	    				val[i]=330;
	    				i++;
	    				a++;
	    			}
	    			for(a=1;a<2;)
	    			{
	    				kg[j]=1;
	    				j++;
	    				a++;
	    			}
