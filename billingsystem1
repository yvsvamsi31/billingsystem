import java.util.Scanner;
public class Shopping {
	static Scanner sc=new Scanner(System.in);
	int finalprice=0;
	static int i=1;
	static int j=1;
	static int k=1;
	static int l=1;
	static int total=0;
	static int[] kg=new int[20];
	static int[] cod=new int[20];
    static String nam[]=new String[200];  
	static int[] val= new int[20] ;
	
		
	
	void Entry()
	{
		
		System.out.println(
				"-------------------------------------------------------------------------------------------------------------------");
				System.out.println(
				"####################################################################################################################");
				System.out.println(
				"-------------------------------------------WELCOME TO JUMANJI STORE-------------------------------------------------");
				System.out.println(
				"####################################################################################################################");
				System.out.println(
			    "-------------------------------------------------------------------------------------------------------------------");
				System.out.println("if you want to see the items please type 1");
				int type=sc.nextInt();
				
				switch(type)
				{
				case 1:
					new Shopping().items();
					break;
				default :
					System.out.println("Incorrect option");
					new Shopping().Entry();
				}
	}
	void items()
	{
		
		System.out.println("1.Fruits ");
	    System.out.println("2.Vegetables");
	    System.out.println("3.Cosmetics");
	    System.out.println("4.Masala Powders");
	    System.out.println("5.Exit");
	    System.out.println("---------------------------------------------------------------------------------------------------------");
	    System.out.println("Please choose your option");
	    int number=sc.nextInt();
	    
	    
	    switch(number)
	    {
	    case 1:
	    	new Shopping().fruits();
	    	break;
	    case 2:
	    	new Shopping().vegetables();
	    	break;
	    case 3:
	    	new Shopping().cosmetics();
	    	break;
	    case 4:
	    	new Shopping().masalaPowders();
	    case 5:
	    	return;
	    	default :
	    		System.out.println("choose the right option");
	    		new Shopping().items();
	    }
				
	}

	void price()
	{
		
		
		
		System.out.println("if you want to continue shopping type 1");
		System.out.println("if you want to stop type 2");
		int type=sc.nextInt();
		switch(type)
		{
		case 1:
		  {
			new Shopping().items();
			break;
		  }
		case 2:
		  {
			int f;
			System.out.println(	"*****************************************************************************************");
			System.out.println( "!!!----------------------------Welcome to Jumanji Store-------------------------------!!!");
			System.out.println( "*****************************************************************************************");
			System.out.println( "-----------------------------------------------------------------------------------------"); 
			System.out.println( " S.No     Product Name           Product Code       Quantity        Price     Total Price");
			System.out.println( " ====     =============           ============       ========      ======    ===========");
			System.out.println( "-----------------------------------------------------------------------------------------");
			for(f=1;f<i;f++)
			{
			System.out.println( "     "+f+"      "+nam[f]+   "       -           "+cod[f]+"       -       "+kg[f]+"      -       "+val[f]+"  -  "+(val[f]*kg[f]));
			System.out.println("-----------------------------------------------------------------------------------------");
			}
			for(f=1;f<=i;f++)
			{
				
				total=total*1+val[f]*kg[f];
			}
			
			System.out.println(" Total bill of purchased items: "+total);
			System.out.println( "<<<<<<<<<<<<<<<<<<<<<<<<<<<<<---Thank you for shopping--->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>");
			System.out.println("*****************************************************************************************");
			
			break;
			
		  }	
		  default:
		  {
			  System.out.println("Please choose the right option------------------------");
			  new Shopping().price();
		  }
	   }
	}
	
